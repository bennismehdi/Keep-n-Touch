<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="co.example.um2.aigle.alo">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher2"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Common.Registration.RegistrationActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Common.Welcome.WelcomeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Common.Configuration.ConfiguratoinActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Common.ServiceActivity"
            android:screenOrientation="portrait"
            android:theme="@style/ThemeService" />
        <activity
            android:name=".Common.News.NewsActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".Common.Commerce.CommerceActivity" />
        <activity android:name=".Common.Commerce.VendreActivity" />
        <activity android:name=".Common.devmobil_tp1.DetailsActivity" />
        <activity
            android:name=".Common.devmobil_tp1.ActualiteActivity"
            android:label="@string/title_activity_actualite" />
        <activity android:name=".Common.devmobil_tp1.ReveilActivity" />
        <!-- suppress AndroidDomInspection -->
        <receiver android:name=".Common.devmobil_tp1.AlarmReceveirActivity" />

        <activity android:name=".Common.Commerce.ItemsDetail"></activity>
    </application>

</manifest>